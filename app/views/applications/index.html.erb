<h1>All Applications</h1>
  <%= link_to "Start An Application", "/applications/new" %>
<h3>

<% @applications.each do |application| %>
  <h5>Name: <%= application.name %></h3>
  <h5>Street Address: <%=application.street_address%> <%=application.city%> <%=application.state%> <%=application.zip_code%> </h5>
  <h5>Description: <%= application.description %></h5>
  <h5>Pet Names: <%= application.pet_names %></h5>
  <h5>Status: <%= application.status %></h5>
<% end %>
