<%= render partial: 'application/header', layout: 'header' %>
<%= render partial: 'application/body', layout: 'body' %>

<div class="myDiv" >
  <h1>Edit Shelter</h1>
</div>

<div class="body" >
<%= form_with url: "/shelters/#{@shelter.id}", method: :patch, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :foster_program %>
  <%= f.check_box :foster_program %>

  <%= f.label :rank %>
  <%= f.number_field :rank %>

  <%= f.submit %>
<% end %>
</div>
